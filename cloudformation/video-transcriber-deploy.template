    Type: String
    AllowedValues:
      - en-AU
      - en-US
      - en-GB
      - es-US
      - en-IN
      - de-DE
      - hi-IN
      - pt-BR
      - fr-CA
      - fr-FR
      - it-IT
      - ta-IN
      - te-IN
      - zh-CN
    Default: zh-CN
